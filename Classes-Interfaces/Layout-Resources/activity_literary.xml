<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bk5"
    tools:context=".Literary">

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/spell_bee" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView25"
        android:layout_marginEnd="32dp"
        android:src="@drawable/g_quiz2" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView25"
        android:layout_below="@+id/imageView25"
        android:text="Spell-Bee" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView26"
        android:layout_below="@+id/imageView25"
        android:text="G-Quiz" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView25"
        android:layout_marginTop="195dp"
        android:src="@drawable/theme_quiz" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignStart="@+id/imageView26"
        android:layout_alignTop="@+id/imageView27"
        android:src="@drawable/debate" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView25"
        android:layout_below="@+id/imageView27"
        android:text="Theme-Quiz" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView26"
        android:layout_below="@+id/imageView27"
        android:text="Debate" />
</RelativeLayout>