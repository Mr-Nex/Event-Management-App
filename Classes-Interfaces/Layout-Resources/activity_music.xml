<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bk5"
    tools:context=".Music">

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="29dp"
        android:src="@drawable/band" />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView37"
        android:layout_marginEnd="30dp"
        android:src="@drawable/beat_box" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:gravity="center"
        android:layout_alignStart="@+id/imageView37"
        android:layout_below="@+id/imageView37"
        android:text="Band" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView38"
        android:layout_below="@+id/imageView37"
        android:text="Beat Box" />

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView37"
        android:layout_marginTop="197dp"
        android:src="@drawable/music_waste" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignStart="@+id/imageView38"
        android:layout_alignTop="@+id/imageView39"
        android:src="@drawable/rap" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView37"
        android:layout_below="@+id/imageView39"
        android:text="Music out of waste" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView38"
        android:layout_below="@+id/imageView39"
        android:text="Rap" />

    <ImageView
        android:id="@+id/imageView41"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_above="@+id/textView41"
        android:layout_alignStart="@+id/imageView37"
        android:src="@drawable/solo_sing" />


    <TextView
        android:id="@+id/textView41"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageView37"
        android:layout_marginBottom="45dp"
        android:text="Solo Singing" />


</RelativeLayout>