<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bk5"
    tools:context=".LifeStyle">


    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="35dp"
        android:src="@drawable/le_senorita"/>

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView22"
        android:layout_marginEnd="30dp"
        android:src="@drawable/fashionista" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="148dp"
        android:layout_height="37dp"
        android:layout_alignStart="@+id/imageView22"
        android:layout_below="@+id/imageView22"
        android:gravity="center"
        android:text="La Senorita"
        android:textColor="#fdd835"
        android:textSize="17dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView23"
        android:layout_below="@+id/imageView22"
        android:text="Fashionista" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView22"
        android:layout_marginTop="215dp"
        android:src="@drawable/lr_senor" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView24"
        android:layout_marginStart="27dp"
        android:text="Le Senor" />
</RelativeLayout>