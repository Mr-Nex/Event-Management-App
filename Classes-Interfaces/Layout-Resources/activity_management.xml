<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bk5"
    tools:context=".Management">

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/ipl" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="148dp"
        android:layout_height="117dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView29"
        android:layout_marginEnd="30dp"
        android:src="@drawable/market2" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView29"
        android:layout_below="@+id/imageView29"
        android:text="Mock IPL" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="148dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:textSize="17dp"
        android:textStyle="bold"
        android:textColor="#fdd835"
        android:layout_alignStart="@+id/imageView30"
        android:layout_below="@+id/imageView29"
        android:text="Market Mayhem" />


</RelativeLayout>